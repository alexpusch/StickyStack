{
  "name": "stickyStack",
  "version": "0.0.1",
  "description": "Make your reading context stick while scrolling",
  "main": "dist/stickyStack.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexpusch/stikcyStack.git"
  },
  "keywords": [
    "scroll",
    "ux"
  ],
  "author": "Alex Puschinsky",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-polyfill": "*",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.0",
    "chai": "^3.5.0",
    "core-js": "^2.0.3",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "imports-loader": "^0.6.5",
    "jquery": "^2.2.3",
